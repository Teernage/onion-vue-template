{
  "manifest_version": 3,
  "name": "onion",
  "version": "0.0.0",
  "description": "欢迎使用 onion-Popup插件",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "img/favicon_16.png",
    "32": "img/favicon_32.png",
    "48": "img/favicon_48.png",
    "128": "img/favicon_128.png"
  },
  "background": {
    "service_worker": "src/BackgroundScript/background.ts"
  },
  "action": {
    "default_popup": "index.html",
    "browser_style": false
  },
  "update_url": "https://yct.oray.com/appstore/service/update/crx"
}
